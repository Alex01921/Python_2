numbers = [5, 2, 1, 7, 4]
print(numbers)
numbers.append(120)
print(numbers)
numbers.remove(5)
print(numbers)
numbers.pop()
print(numbers)
print(50 in numbers)
print(numbers.index(2))  # to find existence of number
print(numbers.count(1))
numbers.sort()
print(numbers)
numbers.reverse()
print(numbers)
numbers2 = numbers.copy()
print(numbers2)
numbers.clear()
print(numbers)
